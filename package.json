{
  "name": "vite-plugin-module-list",
  "private": false,
  "version": "2.0.1",
  "description": "Vite plugin that writes a module that dynamically imports modules found in a folder.",
  "author": "David Bonnet <david@bonnet.cc>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidbonnet/vite-plugin-module-list.git"
  },
  "type": "module",
  "main": "./dist/main.js",
  "exports": {
    "types": "./dist/main.d.ts",
    "import": "./dist/main.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.package.json",
    "build:doc": "typedoc",
    "prepare": "npm run build",
    "test": "tsc"
  },
  "files": [
    "dist",
    "README.md",
    "doc",
    "lib"
  ],
  "keywords": [
    "vite",
    "vite-plugin",
    "module",
    "generator",
    "import"
  ],
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@types/node": "^20.5.7",
    "@types/normalize-path": "^3.0.0",
    "preact": "^10.17.1",
    "typedoc": "^0.25.0",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "normalize-path": "^3.0.0",
    "prettier": "^3.0.3"
  }
}
